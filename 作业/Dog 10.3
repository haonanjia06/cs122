package haonanjia;

public class Dog implements Speaker
{
    private String name;

    
    public Dog(String nameInit)
    {
        name = nameInit;
    }

    
    public void speak()
    {
        System.out.println(name + " says: \"Woof\"");
    }

  
    public void announce(String str)
    {
        System.out.println(name + " announces: \"" + str + "\"");
    }
}
